<template id="progress-ui-template">

    <div class="progress-ui-cont-outer">
        <div class="progress-ui-cont">
            <div class="progress-ui-bar"></div>
            <div class="progress-text-cont">
                <span class="progress-text"></span>&nbsp;<span class="progress-value"></span>
            </div>
        </div>
        <div style="clear:both">&nbsp;</div>
    </div>
    <div class="modal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <div class="progress-ui"></div>                    
                </div>
                <div class="modal-footer"></div>
            </div>
        </div>
    </div>

</template>
